{
  "name": "twitch-greet-newcomers",
  "version": "1.0.4",
  "description": "a bookmarklet to help you spot newcomers in a twitch chat",
  "main": "greet-newcomers.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm run build-external && npm run build-local && npm run build-docs",
    "build-external": "bookmarklet bookmarklet-external.js greet-newcomers-external.bookmarklet",
    "build-local": "bookmarklet greet-newcomers.js greet-newcomers-local.bookmarklet",
    "build-docs": "bookmarklet bookmarklet-external.js docs/_includes/greet-newcomers.bookmarklet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaslule/twitch-greet-newcomers.git"
  },
  "author": "thomaslule",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomaslule/twitch-greet-newcomers/issues"
  },
  "homepage": "https://github.com/thomaslule/twitch-greet-newcomers#readme",
  "devDependencies": {
    "bookmarklet": "^1.0.2"
  },
  "dependencies": {}
}
